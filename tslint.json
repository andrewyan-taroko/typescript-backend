{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-submodule-imports": false,
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "no-shadowed-variable": [true, { "temporalDeadZone": false }]
  },
  "rulesDirectory": []
}
